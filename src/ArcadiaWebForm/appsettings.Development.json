{
  "UseRemote": true,
  "Authentication": {
    "AzureAd": {
      "AADInstance": "https://login.microsoftonline.com/",
      "CallbackPath": "/signin-oidc",
      "ClientId": "[client id from created application in AAD]",
      "TenantId": "[your tenant id]",
      "SecretKey": "[your secret key]",
      "ResourceId": "https://uxrisk.com/api"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Settings": {
    "BaseUrl": "https://api.uxrisk.com"
  }
}
